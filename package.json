{
	"name": "@ctx-core/atob",
	"version": "10.2.39",
	"description": "ctx-core atob",
	"keywords": [
		"ctx-core",
		"atob"
	],
	"homepage": "https://github.com/ctx-core/atob#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/atob/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/atob.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json"
	],
	"types": "./index.d.ts",
	"exports": {
		".": {
			"browser": "./index.browser.js",
			"default": "./index.js",
			"node": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"ctx-core": "^4.11.0"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.6.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}