{
	"name": "@ctx-core/atob",
	"version": "10.0.18",
	"description": "ctx-core atob",
	"keywords": [
		"ctx-core",
		"atob"
	],
	"homepage": "https://github.com/ctx-core/atob#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/atob/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/atob.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"browser": "./dist/index.browser.js",
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^11.0.22",
		"atob-lite": "^2.0.0",
		"esm": "^3.2.25"
	},
	"devDependencies": {
		"@types/atob-lite": "^2.0.0",
		"typescript": "^4.4.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}