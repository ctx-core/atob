{
	"name": "@ctx-core/atob",
	"version": "10.0.32",
	"description": "ctx-core atob",
	"keywords": [
		"ctx-core",
		"atob"
	],
	"homepage": "https://github.com/ctx-core/atob#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/atob/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/atob.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"browser": "./dist/index.browser.js",
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^11.0.40",
		"atob-lite": "^2.0.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"@types/atob-lite": "^2.0.0",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
